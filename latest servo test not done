#include <Servo.h>
double target = 75;

Servo myservo;

void setup() {

 myservo.attach(7);
 myservo.write(90);
 
}

void loop() {
//put your main code here, to run repeatedly:


   Serial.println(target);
   Serial.println(myservo.read());

   if(myservo.read() < 75 && target==75)
   {
    target=105;
   }
   else if(myservo.read() > 105 && target==105)
   {
    target=75;
   }
   myservo.write(target);

   Serial.println(target);
   Serial.println(myservo.read());
   

 
}
